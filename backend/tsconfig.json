{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "esnext",
    "strict": true,
    "resolveJsonModule": true
  },
  "typeRoots": ["./custom_typings", "./node_modules/@types"],
  "exclude": ["node_modules"],
  "types": ["node", "aws-sdk", "aws-lambda", "uuid"]
}
